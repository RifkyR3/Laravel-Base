{
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "build:empty": "run-p type-check build-empty",
        "preview": "vite preview",
        "build-only": "vite build",
        "build-empty": "vite build --emptyOutDir",
        "build-watch": "vite build --watch",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "back:clean:prod": "composer install --optimize-autoloader --no-dev && composer dump-autoload && php artisan view:cache && php artisan route:cache && php artisan config:cache",
        "back:clean:dev": "composer install --optimize-autoloader && composer dump-autoload && php artisan view:cache && php artisan route:cache && php artisan config:cache",
        "back:init:dev": "yarn back:clean:dev && php artisan migrate:fresh --seed",
        "back:init:prod": "yarn back:clean:prod && php artisan migrate",
        "serve": "concurrently \"php artisan serve\" \"yarn dev\""
    },
    "devDependencies": {
        "@originjs/vite-plugin-commonjs": "^1.0.3",
        "@popperjs/core": "^2.11.6",
        "@rushstack/eslint-patch": "^1.2.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@tailwindcss/typography": "^0.5.9",
        "@types/node": "^18.15.1",
        "@vitejs/plugin-legacy": "^4.0.1",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.14",
        "bootstrap": "^5.2.3",
        "eslint": "^8.34.0",
        "eslint-plugin-vue": "^9.9.0",
        "laravel-vite-plugin": "^0.7.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.21",
        "prettier": "^2.8.4",
        "sass": "^1.56.1",
        "tailwindcss": "^3.2.7",
        "typescript": "~4.8.4",
        "vite": "^4.0.0",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-imagemin": "^0.6.1",
        "vite-plugin-minify": "^1.5.2",
        "vue": "^3.2.36",
        "vue-tsc": "^1.2.0"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.12",
        "@heroicons/vue": "^2.0.16",
        "@vueuse/core": "^9.13.0",
        "axios": "^1.1.2",
        "lodash": "^4.17.21",
        "pinia": "^2.0.33",
        "vee-validate": "^4.8.1",
        "vue-loader": "^17.0.1",
        "vue-router": "^4.1.6",
        "yup": "^1.0.2"
    }
}
